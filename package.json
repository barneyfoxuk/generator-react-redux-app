{
  "name":            "generator-react-redux-app",
  "version":         "0.1.1",
  "description":     "Yeoman generator for React with Redux",
  "repository":      "https://github.com/tonypizzicato/generator-react-redux-app",
  "main":            "generators/app/index.js",
  "files":           [
    "generators"
  ],
  "scripts":         {
    "clean":      "rm -rf ./generators",
    "compile":    "npm run clean && ./node_modules/.bin/babel src -d generators -x .es6 -D",
    "prepublish": "npm run compile",
    "test":       "./node_modules/.bin/_mocha"
  },
  "dependencies":    {
    "core-js":          "^2.0.2",
    "es6-promise":      "^3.0.2",
    "isomorphic-fetch": "^2.2.0",
    "lodash":           "^3.10.1",
    "slug":             "^0.9.1",
    "yeoman-generator": "^0.22.2"
  },
  "devDependencies": {
    "babel-cli":            "^6.3.17",
    "babel-core":           "^6.4.0",
    "babel-preset-es2015":  "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "json-loader":          "^0.5.4",
    "mocha":                "^2.3.4",
    "yeoman-assert":        "^2.1.1",
    "yeoman-test":          "^1.0.0"
  },
  "keywords":        [
    "react",
    "redux",
    "webpack",
    "es6",
    "yeoman",
    "yeoman-generator"
  ],
  "author":          "tony.pizzicato",
  "license":         "ISC"
}
